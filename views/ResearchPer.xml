<odoo>

        <record model="ir.ui.view" id="ResearchPer_form_view">
            <field name="name">ResearchPer.form</field>
            <field name="model">NaturalParks.ResearchPer</field>
            <field name="arch" type="xml">
                <form string="ResearchPer Form">
                    <sheet>
                        <group>
                            <field name="name"/>
                            <field name="DNI"/>
                            <field name="SSNumber"/>
                            <field name="Address"/>
                            <field name="MobilePhone"/>
                            <field name="HomePhone"/>
                            <field name="Salary"/>
                            <field name="NaturalParkID"/>
                            <field name="Title"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record model="ir.ui.view" id="ResearchPer_tree_view">
            <field name="name">ResearchPer.tree</field>
            <field name="model">NaturalParks.ResearchPer</field>
            <field name="arch" type="xml">
                <form string="ResearchPer Tree">
                        <group>
                            <field name="name"/>
                            <field name="DNI"/>
                            <field name="SSNumber"/>
                            <field name="Address"/>
                            <field name="MobilePhone"/>
                            <field name="HomePhone"/>
                            <field name="Salary"/>
                            <field name="NaturalParkID"/>
                            <field name="Title"/>
                        </group>
                </form>
            </field>
        </record>

        <record model="ir.ui.view" id="ResearchPer_list_action">
          <field name="name">ResearchPer</field>
          <field name="res_model">NaturalParks.ResearchPer</field>
          <field name="view_mode">form,tree</field>
          <field name="help" type="html">
          </field>
        </record>

        <record model="ir.ui.view" id="ResearchPer_search_view">
         <field name="name">ResearchPer.search</field>
         <field name="model">NaturalParks.ResearchPer</field>
         <field name="arch" type="xml">
             <search>
                 <field name="name"/>
                 <field name="DNI"/>
                 <field name="SSNumber"/>
                 <filter name="HighSalary" string="High Salary"
                         domain="[('Salary', '&gt;', 22220)]"/>
                 <filter name="LowSalary" string="Low Salary"
                         domain="[('Salary', '&lt;', 22220)]"/>
                 <group string="Group By">
                     <filter name="by_NaturalPark" string="Natural Park"
                             context="{'group_by': 'NaturalParkID'}"/>
                 </group>
                 <group string="Group By">
                     <filter name="by_NaturalPark" string="NaturalPark"
                             context="{'group_by': 'NaturalParkID'}"/>
                 </group>
             </search>
         </field>
        </record>

        <menuitem id="ResearchPer_menu" name="ResearchPer"
          parent="ParkPersonal_menu"
          action="ResearchPer_list_action"/>
</odoo>