<odoo>

        <record model="ir.ui.view" id="ManagementPer_form_view">
            <field name="name">ManagementPer.form</field>
            <field name="model">NaturalParks.ManagementPer</field>
            <field name="arch" type="xml">
                <form string="ManagementPer Form">
                    <sheet>
                        <group>
                            <field name="name"/>
                            <field name="DNI"/>
                            <field name="SSNumber"/>
                            <field name="Address"/>
                            <field name="MobilePhone"/>
                            <field name="HomePhone"/>
                            <field name="Salary"/>
                            <field name="NaturalParkID"/>
                            <field name="NumberOfEntry"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record model="ir.ui.view" id="ManagementPer_tree_view">
            <field name="name">ManagementPer.tree</field>
            <field name="model">NaturalParks.ManagementPer</field>
            <field name="arch" type="xml">
                <form string="ManagementPer Tree">
                        <group>
                            <field name="name"/>
                            <field name="DNI"/>
                            <field name="SSNumber"/>
                            <field name="Address"/>
                            <field name="MobilePhone"/>
                            <field name="HomePhone"/>
                            <field name="Salary"/>
                            <field name="NaturalParkID"/>
                            <field name="NumberOfEntry"/>
                        </group>
                </form>
            </field>
        </record>

        <record model="ir.ui.view" id="ManagementPer_list_action">
          <field name="name">ManagementPer</field>
          <field name="res_model">NaturalParks.ManagementPer</field>
          <field name="view_mode">form,tree</field>
          <field name="help" type="html">
          </field>
        </record>

       <record model="ir.ui.view" id="ManagementPer_search_view">
           <field name="name">ManagementPer.search</field>
           <field name="model">NaturalParks.ManagementPer</field>
           <field name="arch" type="xml">
               <search>
                   <field name="name"/>
                   <field name="DNI"/>
                   <field name="SSNumber"/>
                   <filter name="HighSalary" string="High Salary"
                           domain="[('Salary', '&gt;', 20000)]"/>
                   <filter name="LowSalary" string="Low Salary"
                           domain="[('Salary', '&lt;', 20000)]"/>
                   <group string="Group By">
                       <filter name="by_NaturalPark" string="Natural Park"
                               context="{'group_by': 'NaturalParkID'}"/>
               </search>
           </field>
       </record>

       <menuitem id="ManagementPer_menu" name="ManagementPer"
         parent="ParkPersonal_menu"
         action="ManagementPer_list_action"/>
</odoo>