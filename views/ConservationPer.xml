<odoo>

        <record model="ir.ui.view" id="ConservationPer_form_view">
            <field name="name">ConservationPer.form</field>
            <field name="model">NaturalParks.ConservationPer</field>
            <field name="arch" type="xml">
                <form string="ConservationPer Form">
                    <sheet>
                        <group>
                            <field name="name"/>
                            <field name="DNI"/>
                            <field name="SSNumber"/>
                            <field name="Address"/>
                            <field name="MobilePhone"/>
                            <field name="HomePhone"/>
                            <field name="Salary"/>
                            <field name="NaturalParkID"/>
                            <field name="AreaID"/>
                            <field name="Specialty"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record model="ir.ui.view" id="ConservationPer_tree_view">
            <field name="name">ConservationPer.tree</field>
            <field name="model">NaturalParks.ConservationPer</field>
            <field name="arch" type="xml">
                <form string="ConservationPer Tree">
                        <group>
                            <field name="name"/>
                            <field name="DNI"/>
                            <field name="SSNumber"/>
                            <field name="Address"/>
                            <field name="MobilePhone"/>
                            <field name="HomePhone"/>
                            <field name="Salary"/>
                            <field name="NaturalParkID"/>
                            <field name="AreaID"/>
                            <field name="Specialty"/>
                        </group>
                </form>
            </field>
        </record>

        <record model="ir.ui.view" id="ConservationPer_list_action">
          <field name="name">ConservationPer</field>
          <field name="res_model">NaturalParks.ConservationPer</field>
          <field name="view_mode">form,tree</field>
          <field name="help" type="html">
          </field>
       </record>

       <record model="ir.ui.view" id="ConservationPer_search_view">
            <field name="name">ConservationPer.search</field>
            <field name="model">NaturalParks.ConservationPer</field>
            <field name="arch" type="xml">
                <search>
                  <field name="name"/>
                  <field name="DNI"/>
                  <field name="SSNumber"/>
                  <filter name="HighSalary" string="High Salary"
                        domain="[('Salary', '&gt;', 22220)]"/>
                  <filter name="LowSalary" string="Low Salary"
                        domain="[('Salary', '&lt;', 22220)]"/>
                  <group string="Group By">
                      <filter name="by_NaturalPark" string="Natural Park"
                              context="{'group_by': 'NaturalParkID'}"/>
                  </group>
                  <group string="Group By">
                      <filter name="by_Area" string="Area"
                              context="{'group_by': 'AreaID'}"/>
                  </group>
                </search>
            </field>
       </record>

       <menuitem id="ConservationPer_menu" name="ConservationPer"
         parent="ParkPersonal_menu"
         action="ConservationPer_list_action"/>
</odoo>